{
	"key": "",
	"name": "Bing Pong Helper",
	"version": "1.4.4.1",
	"description": "Extends the abilities of Bing Pong",
	"manifest_version": 2,
	"page_action": {
		"default_icon": "bp128.png",
		"default_popup": "popup.html"
	},
	"icons": { 
		"16": "bp16.png",
		"48": "bp48.png",
		"128": "bp128.png" 
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"permissions": [
		"*://*.bing.com/*", "*://*.bing.net/*", "*://*.live.com/*", "*://*.microsoft.com/*", "*://*.msn.com/*", "*://*.wikipedia.org/*", "*://*.brian-kieffer.com/*","webRequest", "webRequestBlocking", "tabs", "cookies", "contentSettings", "power", "storage", "alarms"
	],
	"optional_permissions": [
		"background", "notifications", "proxy"
	],
	"background": {
		"page": "background.html"
	},
	"content_scripts": [
		{
			"matches": ["*://*.bing.com/*", "*://*.bing-pong.com/*", "*://*.bingpong.net/*"],
			"js": ["scripts/script-injector.js"],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"externally_connectable": {
		"matches": ["*://*.bing-pong.com/*", "*://*.bingpong.net/*"]
	},
	"web_accessible_resources": ["scripts/blockalerts.js", "scripts/click-on-search-result.js", "scripts/click-on-second-page.js", "scripts/jquery.js", "scripts/emulation.js", "search_result_blocked.html"],
	"homepage_url": "http://bing-pong.com"
}